				<usage>
					<sections>
						<columns>1</columns>
						<body />
						<sidebar />
					</sections>
					<shortTitle>Usage</shortTitle>
					<highlightJs>true</highlightJs>
				</usage>
			</pages>
		</reflowSkin>
	</custom>

	<body>
		<menu name="Documentation">
			<item name="Add to your project" href="dependency.html" />
